<mxGraphModel dx="913" dy="493" grid="1" gridSize="10" guides="1" tooltips="1" connect="1" arrows="1" fold="1" page="1" pageScale="1" pageWidth="3300" pageHeight="4681" math="0" shadow="0">
  <root>
    <mxCell id="0" />
    <mxCell id="1" parent="0" />
    {% for item in items %}
    {% if item.type == "team" %}
    <mxCell id="{{ item.id }}" value="{{ item.display_name }}" style="{{ configs[item.style] }}" parent="1" vertex="1">
      <mxGeometry x="{{ item.x }}" y="{{ item.y }}" width="{{ item.w }}" height="{{ item.h }}" as="geometry" />
    </mxCell>
    {% endif %}

    {% if item.type == "group" %}
    <mxCell id="{{ item.id }}" value="{{ item.display_name }}" style="rounded=0;whiteSpace=wrap;html=1;fontSize=32;horizontal=1;verticalAlign=top;" parent="1" vertex="1">
      <mxGeometry x="{{ item.x }}" y="{{ item.y }}" width="{{ item.w }}" height="{{ item.h }}" as="geometry" />
    </mxCell>
    {% endif %}

    {% if item.type == "module" %}
    <mxCell id="{{ item.id }}" value="{{ item.display_name }}" style="rounded=0;whiteSpace=wrap;html=1;fontSize=12;strokeColor=#666666;fillColor=#f5f5f5;container=1;collapsible=0;glass=0;comic=0;shadow=0;fontColor=#333333;resizable=0;rotatable=0;" vertex="1" parent="1">
      <mxGeometry x="{{ item.x }}" y="{{ item.y }}" width="180" height="50" as="geometry" />
    </mxCell>
    <mxCell id="{{ item.id }}-1" value="" style="{{ configs[item.wg_stype] }}" vertex="1" parent="{{ item.id }}">
      <mxGeometry width="20" height="50" as="geometry" />
    </mxCell>
    <mxCell id="{{ item.id }}-2" value="" style="{{ configs[item.status] }}" vertex="1" parent="{{ item.id }}">
      <mxGeometry x="160" width="20" height="50" as="geometry" />
    </mxCell>
    {% endif %}

    {% if item.type == "wg" %}
    <mxCell id="{{ item.id }}" value="{{ item.display_name }}" style="{{ configs[item.style] }}" parent="1" vertex="1">
      <mxGeometry x="{{ item.x }}" y="{{ item.y }}" width="{{ item.w }}" height="{{ item.h }}" as="geometry" />
    </mxCell>
    {% endif %}

    {% endfor %}
  </root>
</mxGraphModel>
